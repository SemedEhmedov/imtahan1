@model CreateUserVm
<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="FullName">FullName</label>
            <input asp-for="FullName" class="form-control" id="exampleFormControlInput1" placeholder="FullName">
            <span asp-validation-for="FullName" style="color:red"></span>
        </div>
        <div class="form-group">
            <label asp-for="Descriptrion">Descriptrion</label>
            <input asp-for="Descriptrion" class="form-control" id="exampleFormControlInput1" placeholder="Descriptrion">
            <span asp-validation-for="Descriptrion" style="color:red"></span>
        </div>
        <div class="form-group">
            <label asp-for="Rating">Rating</label>
            <input asp-for="Rating" class="form-control" id="exampleFormControlInput1" placeholder="Rating">
            <a>Rating 5-den boyuk ola bilmez</a>
            <span asp-validation-for="Rating" style="color:red"></span>
        </div>
        <div class="form-group">
            <label asp-for="WebSiteId">WebSite select</label>
            <select asp-for="WebSiteId" asp-items=@(new SelectList(ViewBag.WebSites,"Id","Name")) class="form-control" id="exampleFormControlSelect1">
                <option>Select WebSite</option>

            </select>
        </div>
        <div class="form-group">
            <label asp-for="File">Image</label>
            <input asp-for="File" class="form-control" id="exampleFormControlInput1" placeholder="Image">
            <span asp-validation-for="File" style="color:red"></span>
        </div>
        <button class="btn btn-primary">Submit</button>
    </form>
</div>
